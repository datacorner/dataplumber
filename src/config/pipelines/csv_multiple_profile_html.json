{
    "classname" : "pipelite.pipelines.sequentialPL",
    "extractors" : [ 
        {
        "id": "Source A",
        "classname": "pipelite.datasources.csvFileDS",
        "parameters": {
            "separator": ",",
            "filename": "a.csv",
            "path": "tests/data/",
            "encoding": "utf-8"
        }   
    },
    {
        "id": "process activite",
        "classname": "pipelite.datasources.csvFileDS",
        "parameters": {
            "separator": ";",
            "filename": "Data_poc_process_activite (1) (1 LIGHT).csv",
            "path": "tests/data/private/",
            "encoding": "utf-8"
        }   
    }  
    ],
    "transformers":  [     
    { 
        "id": "T",
        "classname": "pipelite.transformers.profileTR",
        "inputs" : [ "Source A", "process activite" ],
        "parameters": {
            "directory" : "tests/data/out",
            "maxvaluecounts": 10,
            "output": "html"
        }
    }],
    "config": {
        "logger" : {
            "level": "DEBUG",
            "format" : "%(asctime)s|%(name)s|%(levelname)s|%(message)s",
            "path": "logs/",
            "filename" : "pipe_multiple_profile_html.log",
            "maxbytes" : 1000000
        }
    }
} 