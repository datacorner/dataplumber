{
    "classname" : "pipelines.directPL",
    "extractors" : [ {
        "name": "S1",
        "classname": "datasources.xesFileDS",
        "validation": "src/config/parameters/datasources/xesFileDS.json",
        "parameters": {
            "filename": "test.xes",
            "path": "tests/data/"
        }   
    } ],
    "loaders" : [ {
        "name": "S1",
        "classname": "datasources.csvFileDS",
        "validation": "src/config/parameters/datasources/csvFileDS.json",
        "parameters": {
            "separator": ",",
            "filename": "test-xes.csv",
            "path": "tests/data/out",
            "encoding": "utf-8"
        }   
    } ],
    "transformers":  [
    { 
        "name": "T",
        "classname": "transformers.donothingTR",
        "inputs" : [ "S1" ],
        "outputs" : [ "S1" ]
    } ],
    "parameters":{
        "mode": "full"
    },
    "config": {
        "logger" : {
            "level": "DEBUG",
            "format" : "%(asctime)s|%(name)s|%(levelname)s|%(message)s",
            "path": "logs/",
            "filename" : "xes2csv_direct.log",
            "maxbytes" : 1000000
        }
    }
} 