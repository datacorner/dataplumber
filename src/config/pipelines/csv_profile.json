{
    "classname" : "pipelite.pipelines.sequentialPL",
    "extractors" : [ {
        "id": "S",
        "classname": "pipelite.datasources.csvFileDS",
        "parameters": {
            "separator": ",",
            "filename": "test.csv",
            "path": "tests/data/",
            "encoding": "utf-8"
        }   
    } ],
    "transformers":  [     
    { 
        "id": "T",
        "classname": "pipelite.transformers.profileTR",
        "inputs" : [ "S" ],
        "outputs" : [ ],
        "parameters": {
            "filename" : "tests/data/a.profile",
            "maxvaluecounts": 20
        }
    }],
    "config": {
        "logger" : {
            "level": "DEBUG",
            "format" : "%(asctime)s|%(name)s|%(levelname)s|%(message)s",
            "path": "logs/",
            "filename" : "pipe_csv2csv.log",
            "maxbytes" : 1000000
        }
    }
} 