{
    "classname" : "pipelines.category.directPipeline",
    "extractors" : [ {
        "name": "E1",
        "classname": "pipelines.datasources.csvFileDS",
        "parameters": {
            "separator": ",",
            "filename": "test.csv",
            "path": "tests/data/",
            "encoding": "utf-8"
        }   
    } ],
    "loaders" : [ {
        "name": "L1",
        "classname": "pipelines.datasources.csvFileDS",
        "parameters": {
            "separator": ",",
            "filename": "test2.csv",
            "path": "tests/data/",
            "encoding": "utf-8"
        }   
    } ],
    "transformers":  [
    { 
        "name": "T2",
        "classname": "pipelines.transformers.doNothingTR",
        "extractors" : [ "E1" ],
        "loaders" : [ "L1" ]
    } ],
    "parameters":{
        "mode": "full"
    },
    "config": {
        "logger" : {
            "level": "DEBUG",
            "format" : "%(asctime)s|%(name)s|%(levelname)s|%(message)s",
            "path": "logs/",
            "filename" : "pipe_csv2csv.log",
            "maxbytes" : 1000000
        }
    }
} 