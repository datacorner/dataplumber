{
    "classname" : "pipelines.category.directPipeline",
    "extractors" : [ {
        "classname": "pipelines.datasources.CSVFileDS",
        "parameters": {
            "separator": ",",
            "filename": "test2.csv",
            "path": "/tests/data/",
            "encoding": "utf-8"
        }   
    } ],
    "loaders" : [ {
        "classname": "pipelines.datasources.CSVFileDS",
        "parameters": {
            "separator": ",",
            "filename": "test.csv",
            "path": "/tests/data/",
            "encoding": "utf-8"
        }   
    } ],
    "transformers":  [ { 
        "classname": "pipelines.transformers.eventMappingTransformer", 
        "parameters" : {
            "maptable" : "./test/chorus-evtmap.csv", 
            "mapcolumn" : "FLOWOBJECTNAME"}
    } ],
    "parameters":{
        "mode": "full"
    },
    "config": {
        "logger" : {
            "level": "DEBUG",
            "format" : "%(asctime)s|%(name)s|%(levelname)s|%(message)s",
            "path": "logs/",
            "filename" : "pipe_csv2csv.log",
            "maxbytes" : 1000000
        }
    }
} 